{{!-- This page will return "My Trips" (when logged in; redirects to login/signup if not" --}}
<div class="row">
  <div class="col-auto">
    <h2>My Trips</h2>
  </div>
</div>
{{#each trips as |trip|}}
<div class="row">
  <div class="col">
    <h3>
      {{trip.name}}
    </h3>
  </div>
  <div class="col">
    <p>
      Start Date: {{trip.start_date}}
    </p>
    <p>
      End Date: {{trip.end_date}}
    </p>
  </div>
  <div class="col">

  </div>
</div>
{{/each}}
<script src="/js/dashboard.js"></script>